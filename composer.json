{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "CERN Drupal Admins",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }        
    ],
    "config": {
        "sort-packages": true,
        "gitlab-domains": ["gitlab.cern.ch"]
    },
    "require": {
        "composer/installers": "1.7.0",
        "cweagans/composer-patches": "1.6.6",
        "drupal/adminimal_theme":"1.4",
        "drupal/admin_toolbar": "1.27",
        "drupal/allowed_formats": "1.1",
        "drupal/bootstrap": "3.20",
        "drupal/captcha": "1.0-beta1",
        "drupal/ckeditor_font": "1.0",
        "drupal/codesnippet": "1.6",
        "drupal/colorbutton": "1.1",
        "drupal/color_field": "2.0",
        "drupal/components": "1.1.0",
        "drupal/config_update": "1.6",
        "drupal/content_access": "1.0-alpha1",
        "drupal/context": "4.0-beta2",
        "drupal/contribute": "5.0.0-beta3",
        "drupal/console": "1.9.1",
        "drupal/cookieconsent": "1.4",
        "drupal/core": "8.7.5",
        "drupal/ctools": "3.2",
        "drupal/devel": "2.1.0",
        "drupal/ds": "3.3",
        "drupal/easy_breadcrumb": "1.12",
        "drupal/entity_browser": "2.2",
        "drupal/entity_print": "2.1",
        "drupal/entity_reference_revisions": "1.6",
        "drupal/externalauth": "1.1",
        "drupal/extlink": "1.1",
        "drupal/fast_404": "1.0-alpha4",
        "drupal/features": "3.8",
        "drupal/feeds": "3.0-alpha5",
        "drupal/fences": "2.0-alpha1",
        "drupal/field_formatter_class": "1.1",
        "drupal/field_group": "1.0",
        "drupal/field_permissions": "1.0-rc2",
        "drupal/filefield_paths": "1.0-beta1",
        "drupal/hotjar": "1.5",
        "drupal/imce": "1.7",
        "drupal/matomo": "1.9",
        "drupal/menu_block": "1.5",
        "drupal/menu_breadcrumb": "1.9",
        "drupal/menu_force": "1.0",
        "drupal/metatag": "1.8",
        "drupal/migrate_plus": "4.2",
        "drupal/migrate_tools": "4.1",
        "drupal/migrate_upgrade": "3.0",
        "drupal/module_filter": "3.1",
        "drupal/node_view_permissions": "1.2",
        "drupal/openid_connect": "1.0",
        "drupal/panelbutton": "1.2",
        "drupal/panelizer": "4.1",
        "drupal/panels": "4.4",
        "drupal/paragraphs": "1.8",
        "drupal/pathauto": "1.4",
        "drupal/piwik": "1.4",
        "drupal/recaptcha": "2.4",
        "drupal/redirect": "1.4",
        "drupal/redis": "1.1",
        "drupal/require_login": "2.1",
        "drupal/rules": "3.0-alpha4",
        "drupal/scheduler": "1.0",
        "drupal/search_api": "1.14",
        "drupal/simplesamlphp_auth": "3.0",
        "drupal/smart_trim": "1.1",
        "drupal/token": "1.5",
        "drupal/twig_tweak": "2.4",
        "drupal/typed_data": "1.0-alpha3",
        "drupal/ui_patterns": "1.1",
        "drupal/userprotect": "1.0",
        "drupal/views_bulk_operations": "3.2",
        "drupal/viewsreference": "2.0-alpha4",
        "drupal/views_slideshow": "4.6",
        "drupal/webform": "5.2-rc3",
        "drupal/workbench": "1.1",
        "drupal/workbench_access": "1.0-beta2",
        "drupal/workbench_moderation": "1.5",
        "drupal-composer/drupal-scaffold": "2.6.1",
        "drush/drush": "9.7.1",
        "erik-dubbelboer/php-redis-admin": "1.11.3",
        "onlyextart/panelbutton": "dev-master",
        "predis/predis": "1.1.x-dev",
        "simplesamlphp/simplesamlphp": "1.17.4",
        "vlucas/phpdotenv": "3.4",
        "webflo/drupal-finder": "1.1.0",
        "webmozart/path-util": "2.3.0",
        "wikimedia/composer-merge-plugin": "1.4.1",
        "zaporylie/composer-drupal-optimizations": "1.1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "Move module/theme installation relative path": "patches/no-multisite-modules-themes-install-2718105-3.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "composer.user.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": true,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        }       
    }
}
